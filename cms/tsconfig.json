{
  "compilerOptions": {
    "target": "es2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "deskStructure.js",
    "src/MyVideoString.jsx",
    "src/MyIframeEmbed.jsx",
    "src/MyAnchorPodcastString.jsx"
  ],
  "exclude": ["node_modules"]
}
